---
import { getCollection } from "astro:content";
import Layout from "../layouts/Layout.astro";

const members = await getCollection("member");
---

<Layout title="Inicio">
  <h1 class="text-4xl">🧑‍🚀 Hello, Astronaut!</h1>

  <h2>🥇 Miembros</h2>

  <ul>
    {members.map((member) => <li>{member.data.name}</li>)}
  </ul>
</Layout>
